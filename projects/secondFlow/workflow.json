{
  "stateMachine": {
    "id": "dockerScript",
    "initial": "setup",
    "context": {},
    "states": {
      "setup": {
        "entry": [
          {
            "type": "createNetwork",
            "networkName": "recon2"
          }
        ],
        "on": {
          "NEXT": "nginxImage2"
        }
      },
      "nginxImage2": {
        "entry": [
          {
            "type": "createContainer",
            "await": true,
            "containerName": "nginx2",
            "dockerFolderName": "nginxImage2",
            "ExposedPorts": {"8083/tcp": {}},
            "PortBindings": { "8083/tcp": [{ "HostPort": "8083" }]},
            "networkName": "recon2"
          }
        ],
        "on": {
          "NEXT": "complete"
        }
      },
      "complete": {
        "type": "final"
      }
    }
  }

}