{
  "stateMachine": {
    "id": "dockerScript",
    "initial": "juiceshop",
    "context": {},
    "states": {
      "juiceshop": {
        "entry": [
          {
            "type": "createContainer",
            "containerName": "localowaspjuiceshop",
            "dockerFolderName": "juiceshop",
            "networkName": "scannetwork",
            "ipaddress": "172.22.0.23",
            "ExposedPorts": {
              "3000/tcp": {}
            },
            "PortBindings": {
              "3000/tcp": [
                {
                  "HostPort": "80"
                }
              ]
            },
            "awaitStart": true
          }
        ],
        "on": {
          "NEXT": "katana"
        }
      },
      "katana": {
        "entry": [
          {
            "type": "createContainer",
            "await": true,
            "containerName": "katana",
            "dockerFolderName": "katana",
            "networkName": "scannetwork",
            "Binds": ["scanlogs:/data/scanlogs"],
            "ExposedPorts": {
              "3000/tcp": {}
            },
            "PortBindings": {
              "3000/tcp": [
                {
                  "HostPort": "3001"
                }
              ]
            }
          }
        ],
        "on": {
          "NEXT": "nuclei"
        }
      },
      "nuclei": {
        "entry": [
          {
            "type": "createContainer",
            "await": true,
            "sustain": true,
            "containerName": "nuclei",
            "dockerFolderName": "nuclei",
            "networkName": "scannetwork",
            "Binds": ["scanlogs:/data/scanlogs"],
            "ExposedPorts": {
              "3000/tcp": {}
            },
            "PortBindings": {
              "3000/tcp": [
                {
                  "HostPort": "3002"
                }
              ]
            }
          }
        ],
        "on": {
          "NEXT": "complete"
        }
      },
      "complete": {
        "type": "final"
      }
    }
  }

}