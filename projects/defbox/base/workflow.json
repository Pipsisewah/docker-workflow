{
  "stateMachine": {
    "id": "dockerScript",
    "initial": "setupNetwork",
    "context": {},
    "states": {
      "setupNetwork": {
        "entry": [
          {
            "type": "createNetwork",
            "networkName": "attacknetwork",
            "subnetMask": "172.22.0.0/16",
            "persist": true
          }
        ],
        "on": {
          "NEXT": "ssrf"
        }
      },
      "ssrf": {
        "entry": [
          {
            "type": "runWorkflow",
            "workflowName": "idor",
            "source": "externalProjects/defbox"
          }
        ],
        "on": {
          "NEXT": "complete"
        }
      },
      "complete": {
        "type": "final"
      }
    }
  }

}